             x:Class="DevTest.App">
    <prism:PrismApplication.Resources>
        <ResourceDictionary Source="Assets/Colors.xaml" />

        <Style TargetType="NavigationPage">
            <Setter Property="BarBackgroundColor" Value="{StaticResource OrangeColor}" />
        </Style>
    </prism:PrismApplication.Resources>
</prism:PrismApplication>